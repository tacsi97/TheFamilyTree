<UserControl x:Class="FamilyTree.Modules.Relationship.Views.RelationshipFunctions"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <StackPanel Orientation="Horizontal">
        <Button Width="100" Height="75" Content="Módosít"
                Command="{Binding EditRelationshipCommand}"/>
        <Button Width="100" Height="75" Content="Töröl"
                Command="{Binding DeleteRelationshipCommand}"/>
    </StackPanel>
</UserControl>
